<!DOCTYPE HTML>
<html>
<head>
<title>FizzBuzz using VBScript</title>
<script language="VBScript">
  Option Explicit

  Sub Fizz_Buzz
    Dim i
    Dim output_str
    output_str = ""
    for i = 1 to 100
      if i mod 3 = 0 then
        output_str  = output_str & "Fizz"
      end if
      if i mod 5 = 0 then
        output_str  = output_str & "Buzz"
      end if
      if i mod 3 <> 0 and i mod 5 <> 0 then
        output_str  = output_str & i
      end if
      output_str  = output_str & "<br>"
    next
    Document.getElementById("output_area").innerHTML = output_str
  End Sub
</script>
</head>
<body>
<div>
<button onclick="Fizz_Buzz">Run FizzBuzz</button>
<div>
<div id="output_area"></div>
</body>
</html>
